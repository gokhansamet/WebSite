<%- include('./partials/header') %>
  <h1>Home</h1>
  <p>
    <%= startContent %>
  </p>
  <% posts.forEach(function (element) { %>
    <div>
      <h1>
        <%= element.title %>
      </h1>
      <p>
        <%= element.content.substring(0,100) + "..." %>
          <a href="./post/<%= element.title %> ">Read More</a>
      </p>
    </div>
    <% }); %>
      <%- include('./partials/footer') %>